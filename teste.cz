FizzBuzz {
  var inteiro numero;
  var inteiro contador = 0;

  leia: inteiro, numero;

  enquanto: (contador <= numero) faca {
    se (numero % 3 == 0 && numero % 5 == 0) entao {
      imprima: "FizzBuzz";
    } se nao se (numero % 3 == 0) entao {
      imprima: "Fizz";
    } se nao se (numero % 5 == 0) entao {
      imprima: "Buzz";
    } se nao entao {
      imprima: "Numero :" + numero;
    }
    contador += 1;
  }
}